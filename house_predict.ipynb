import pandas as pd
import numpy as np

pd.set_option('display.max_columns', None)
pd.set_option('display.max_rows', None)

df = pd.read_csv("housedata.csv")
df.drop(['SizeRank', 'RegionType', 'RegionName', 'StateName', 'Metro', 'CountyName'], axis=1, inplace=True)
cols = list(df.columns)
#df.dropna(inplace=True)
rows = df.shape[0]
unpivoted_df = df.melt(id_vars=list(df.columns)[0:3], value_vars=list(df.columns)[3:], var_name='Date', value_name='HousePrice')
unpivoted_df['Date'] = pd.to_datetime(unpivoted_df['Date'])
unpivoted_df = unpivoted_df.sort_values(['RegionID', 'Date'])
unpivoted_df.set_index('Date', inplace=True)
for row in range(rows):
  for idx, col in enumerate(cols):
    if col in ['RegionID', 'State', 'City']:
      continue
    if pd.isna(df.at[row, col]):
      while(pd.isna(df.at[row, col])):
        try:
          df.loc[row, col] = np.nanmin(df.iloc[row, idx+1:].values)
        except:
          df.loc[row, col] = np.nanmax(df.iloc[row, :idx].values)
        print(f'Replaced NA at row {row} and col {idx} with value {df.at[row, col]}')
import matplotlib.pyplot as plt 
from statsmodels.tsa.seasonal import seasonal_decompose 

interested_zip_code = 91733
arima_df = unpivoted_df.loc[unpivoted_df['RegionID'] == interested_zip_code]

result = seasonal_decompose(arima_df['HousePrice'],  
                            model ='multiplicative') 
result.plot()
from pmdarima import auto_arima 
  
# Ignore harmless warnings 
import warnings 
warnings.filterwarnings("ignore") 
  
# Fit auto_arima function to the dataset 
stepwise_fit = auto_arima(arima_df['HousePrice'], start_p = 1, start_q = 1, 
                          max_p = 3, max_q = 3, m = 12, 
                          start_P = 0, seasonal = True, 
                          d = None, D = 1, trace = True, 
                          error_action ='ignore',   # we don't want to know if an order does not work 
                          suppress_warnings = True,  # we don't want convergence warnings 
                          stepwise = True)           # set to stepwise 
  
# To print the summary 
stepwise_fit.summary()
train = arima_df.iloc[:len(arima_df)-12] 
test = arima_df.iloc[len(arima_df)-12:] # set one year(12 months) for testing
# Fit a SARIMAX on the training set 
from statsmodels.tsa.statespace.sarimax import SARIMAX
  
model = SARIMAX(train['HousePrice'],  
                order = (1, 0, 1),  
                seasonal_order =(1, 0, 1, 12)) 
  
result = model.fit() 
result.summary()
start = len(train) 
end = len(train) + len(test) - 1
  
# Predictions for one-year against the test set 
predictions = result.predict(start, end, 
                             typ = 'levels').rename("Predictions")
# plot predictions and actual values 
predictions.plot(legend = True) 
test['HousePrice'].plot(legend = True)

from sklearn.metrics import mean_squared_error 
from statsmodels.tools.eval_measures import rmse 
  
# Calculate root mean squared error 
print("RMSE:", rmse(test["HousePrice"], predictions))
  
# Calculate mean squared error 
print("MSE:", mean_squared_error(test["HousePrice"], predictions))
